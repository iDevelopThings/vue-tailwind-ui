{
  "name": "@idevelopthings/vue-tailwind-ui",
  "type": "module",
  "description": "Some vue/tailwind/inertia stuff I seem to be copying from project to project.",
  "author": {
    "name": "iDevelopThings",
    "url": "https://github.com/idevelopthings"
  },
  "readme": "readme.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/idevelopthings/vue-tailwind-ui"
  },
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "dist/index.js",
    "./tailwind-plugin": {
      "import": "./tailwind-plugin.cjs",
      "require": "./tailwind-plugin.cjs"
    },
    "./css": {
      "import": "./src/index.css",
      "require": "./src/index.css"
    }
  },
  "files": [
    "dist/**/*",
    "tailwind-plugin.cjs",
    "src/index.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.11",
    "postcss-import": "^15.0.0",
    "vue": "^3.2.40"
  },
  "optionalDependencies": {
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-plugin-build": "^0.6.0",
    "vite-plugin-dts": "^1.6.4"
  }
}
